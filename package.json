  "devDependencies": {
    "@rsbuild/core": "1.1.12",
    "@rsbuild/plugin-vue": "^1.0.5",
    "@rsbuild/plugin-node-polyfill": "1.2.0",
    "@rslib/core": "^0.1.5",
    "rspress": "^1.38.0"
  },
  "dependencies": {
    "@shikijs/markdown-it": "^1.24.2",
