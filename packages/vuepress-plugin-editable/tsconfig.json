{
  "extends": "tsconfig-vuepress/base.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": ["DOM", "ES2022"],
    "isolatedModules":true,
    "module": "ESNext",
    "outDir": "dist",
    "moduleResolution": "bundler",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "rootDir": "./src",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "target": "ES2022"
  },
  "include": ["./src"]
}
