{
  "extends": "tsconfig-vuepress/base.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": ["DOM", "ES2022"],
    "module": "ESNext",
    "outDir": "dist",
    "moduleResolution": "bundler",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    // "types": ["vuepress/client-types"],
    "target": "ES2022",
  },
  "include": ["src"]
}
